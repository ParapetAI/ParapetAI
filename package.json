{
  "name": "@parapetai/parapetai",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*",
    "libs/*"
  ],
  "exports": {
    "./runtime/*": "./dist/runtime/*.js",
    "./config/*": "./dist/config/*.js",
    "./providers/*": "./dist/providers/*.js"
  },
  "scripts": {
    "build": "pnpm -r build",
    "start:runtime": "pnpm --filter @parapetai/runtime run dev",
    "build:runtime:ws": "pnpm --filter @parapetai/runtime run build",
    "compose:up": "docker compose up -d --build",
    "compose:down": "docker compose down",
    "compose:logs": "docker compose logs -f",
    "compose:ps": "docker compose ps",
    "script:openai-sdk-tester": "npx tsx .\\apps\\runtime\\scripts\\openai-sdk-tester.ts",
    "script:descrypt-bootstrap-state": "npx tsx .\\packages\\cli\\scripts\\descrypt-bootstrap-state.ts",
    "script:webhook-receiver": "npx tsx .\\apps\\runtime\\scripts\\webhook-receiver.ts",
    "parapet:build-config": "npx -y tsx packages/cli/src/main.ts build-config -o .env",
    "test:runtime": "pnpm --filter @parapetai/runtime run test"
  },
  "devDependencies": {
    "@types/node": "^22.18.13",
    "openai": "^6.7.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "better-sqlite3": "^12.4.1",
    "fastify": "^5.6.1",
    "yaml": "^2.6.0"
  }
}
